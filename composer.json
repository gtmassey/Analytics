{
    "name": "garrettmassey/analytics",
    "description": "Create and run Google Analytics Data API queries in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Garrett Massey",
            "email": "contact@garrettmassey.net",
            "role": "Creator"
        }
    ],
    "homepage": "https://github.com/gtmassey/Analytics",
    "keywords": [
        "Laravel",
        "Analytics"
    ],
    "require": {
        "google/analytics-data": "^0.9.0",
        "illuminate/support": "~9",
        "nesbot/carbon": "^2.63",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": ">=3.8"
    },
    "autoload": {
        "psr-4": {
            "GarrettMassey\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GarrettMassey\\Analytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyze": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GarrettMassey\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "GarrettMassey\\Analytics\\Facades\\Analytics"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
