{
    "name": "garrettmassey/analytics",
    "description": "Create and run Google Analytics Data API queries in Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Garrett Massey",
            "email": "contact@garrettmassey.net",
            "role": "Creator"
        }
    ],
    "homepage": "https://github.com/gtmassey/Analytics",
    "keywords": [
        "Laravel",
        "Analytics"
    ],
    "require": {
        "google/analytics-data": "^0.9.0",
        "illuminate/support": "~9",
        "nesbot/carbon": "^2.63",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "brianium/paratest": "^6.6",
        "laravel/pint": "^1.2",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/larastan": "^2.2",
        "orchestra/testbench": "^7.13",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "GarrettMassey\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GarrettMassey\\Analytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyze": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/testbench package:test --parallel",
        "pint": "./vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GarrettMassey\\Analytics\\AnalyticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
